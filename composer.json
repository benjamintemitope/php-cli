{
    "name": "benjamintemitope/php-cli",
    "description": "A simple command line tool for interacting with your PHP projects",
    "homepage": "https://github.com/php-cli",
    "keywords": [
        "php",
        "console",
        "installer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Temitope",
            "email": "benjaminfranklin085@gmail.com",
            "homepage": "https://github.com/benjamintemitope",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "psy/psysh": "^0.11.1",
        "symfony/console": "^5.0",
        "symfony/process": "^5.0"
    },
    "bin": [
        "bin/bee"
    ],
    "autoload": {
        "psr-4": {
            "Console\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}